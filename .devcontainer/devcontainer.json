{
  "name": "Airflow Dev Container",
  "dockerComposeFile": [
    "../docker-compose.yaml",
    "docker-compose.override.yml"
  ],
  "service": "airflow-webserver",
  "workspaceFolder": "/opt/airflow",
  "settings": {
    "python.pythonPath": "/home/airflow/.local/bin/python"
  },
  "extensions": [
    "ms-python.python",
    "ms-python.vscode-pylance"
  ],
  "remoteUser": "airflow"
}
